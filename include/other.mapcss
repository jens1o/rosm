    fill-image: "symbols/marsh.png";
    z-index: -3;  /* Custom Z-index for this overlay */
}

way|z13-[waterway][name] {
	z-index: -3;
	text-color: white;
	text-position: line;
	font-size: 10;
	text: name;
	text-color: #6699cc;
    text-halo-radius: 1;
	text-halo-color: white;
    text-position: line;
}

/* ===== Landuse - Buildings ===== */

/* Z-Index for all Buildings */
area[building],
area[aeroway=terminal] {
	z-index: -4;
}

area|z12-[building] {
	fill-color: #bca9a9;
	fill-opacity: 0.9;
}

area|z16-[building!=residential][building!=house][building!=garage][building!=garages][building!=detached][building!=terrace][building!=apartments][building][building!=no][building!=station][building!=supermarket][railway!=station][amenity!=place_of_worship] {
