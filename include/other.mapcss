    fill-color: #cc99ff;
}

area|z14-[aeroway=terminal] {
    color: #330066;
    width: 0.2;
}

/* ==== Railways === */
way|z12-14[railway=rail]												{ color: darkgray;  z-index: 3; width: 1; }
way|z15-[railway=rail]												{ color: darkgray;  z-index: 3; width: 1; dashes: 5,3; }

